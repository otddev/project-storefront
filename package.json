{
  "name": "project-storefront",
  "version": "1.0.0",
  "description": "This is project of a store API engine for udacity course submission.",
  "main": "server.ts",
  "scripts": {
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "export ENV=test && db-migrate --env test down --count 6 && db-migrate --env test up && jasmine-ts --config jasmine.json && db-migrate --env test down --count 6",
    "lint": "eslint src/ --ext .ts,.tsx",
    "build": "tsc -p tsconfig.json",
    "start": "nodemon --ext ts --delay 100ms src/server.ts "
  },
  "author": "Antonio G. Garcia",
  "license": "ISC",
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-winston": "^4.2.0",
    "http-terminator": "^3.0.4",
    "install": "^0.13.0",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "typescript": "^4.1.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/pg": "^7.14.7",
    "@types/express": "^4.17.9",
    "supertest": "^6.1.6",
    "@types/bcrypt": "^5.0.0",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.2.1",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.2.9"
  }
}
